# MySQL Configuration for CloudStack
# Managed by Ansible - Do not edit manually
[mysqld]
# Server ID required for binary logging on Ubuntu 16.04+
server_id={{ prepare_management_mysql_server_id }}
innodb_rollback_on_timeout=1
innodb_lock_wait_timeout=600
max_connections={{ prepare_management_mysql_max_connections }}
log_bin=mysql-bin
binlog_format=ROW
